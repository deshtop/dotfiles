Config { font = "xft:DejaVu Sans Mono:pixelsize=14"
         , bgColor = "#282828"
         , fgColor = "#EBDBB2"
         , lowerOnStart = False
         , commands = [ Run Cpu ["-L","20","-H","50","-l","#B8BB26","-n","#FE8019","-h","#FB4934"] 10
