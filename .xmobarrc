--                    , Run Brightness ["-t","B: <percent>", "-L","50","-H","80","-l","#839496","-n","#93a1a1","-h","#eee8d5"] 5
                      , Run Com "/home/andi/.xmonad/script/bright.sh" [] "bright" 5
